@echo off

rem Run script for SCADASystem

rem Get the directory where the script is located
set SCRIPT_DIR=%~dp0

rem Set PATH to include the lib directory
set PATH=%SCRIPT_DIR%lib;%PATH%

rem Set QML_IMPORT_PATH to include the qml directory
set QML_IMPORT_PATH=%SCRIPT_DIR%qml;%QML_IMPORT_PATH%

rem Set QT_PLUGIN_PATH to include the plugins directory
set QT_PLUGIN_PATH=%SCRIPT_DIR%plugins;%QT_PLUGIN_PATH%

rem Run the application
"%SCRIPT_DIR%SCADASystem.exe"
